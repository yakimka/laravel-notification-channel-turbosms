{
    "name": "yakimka/laravel-notification-channel-turbosms",
    "description": "Turbo SMS Notification channel for Laravel 5.3+.",
    "keywords": ["laravel", "notifications", "turbosms"],
    "homepage": "https://github.com/yakimka/laravel-notification-channel-turbosms",
    "license": "MIT",
    "authors": [
        {
            "name": "yakimka",
            "email": "ss.yakim@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/yakimka"
        },
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com",
            "homepage": "https://github.com/jhaoda"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "5.1 - 5.8|^6.0|^7.0|^8.0",
        "illuminate/support": "5.1 - 5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\TurboSms\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\TurboSms\\TurboSmsServiceProvider"
            ]
        }
    }
}
